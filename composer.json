{
	"name": "sunaryohadi/slim-api-skeleton",
	"description": "A Slim Framework skeleton application for API development",
	"keywords": ["microframework", "rest", "router", "psr7", "api"],
	"homepage": "http://github.com/sunaryohadi/slim-api-skeleton",
	"version": "1.0.0-dev", 
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Sunaryo Hadi",
			"homepage": "https://www.sunaryohadi.info/"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"slim/slim": "^3.5",
		"monolog/monolog": "^1.21",
		"sunaryohadi/ezsql-mysqli": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": ">=5.5 < 6.0"
	},
	"config": {
		"vendor-dir": "private/vendor"
	},
	"autoload": {
		"psr-4": {
			"App\\": "private/src"
		}
	},
	"scripts": {
		"start": "php -S 0.0.0.0:8080 -t www index.php"
	}

}
